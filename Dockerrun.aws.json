{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "sohan0077/proman_frontend:latest",
      "hostname": "client",
      "essential": false,
      "memory": 200
    },
    {
      "name": "backend",
      "image": "sohan0077/proman_backend:latest",
      "hostname": "backend",
      "essential": false,
      "memory": 700
    },
    {
      "name": "nginx",
      "image": "sohan0077/proman_nginx:latest",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],

      "links": ["client", "backend"],
      "memory": 100
    }
  ]
}
