{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "nginx",
      "image": "docker push sohan0077/proman-prod-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["client", "backend"],
      "memory": 128
    },
    {
      "name": "client",
      "image": "docker push sohan0077/proman-prod-frontend",
      "hostname": "client",
      "essential": false,
      "memory": 128
    },
    {
      "name": "backend",
      "image": "docker push sohan0077/proman-prod-django",
      "hostname": "backend",
      "essential": false,
      "memory": 128
    }
  ]
}
