{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "backend",
      "image": "215680092654.dkr.ecr.ap-south-1.amazonaws.com/proman-backend:latest",
      "hostname": "backend",
      "essential": false,
      "memory": 500
    },
    {
      "name": "client",
      "image": "215680092654.dkr.ecr.ap-south-1.amazonaws.com/proman-frontend:latest",
      "hostname": "client",
      "essential": false,
      "memory": 200
    },
    {
      "name": "nginx",
      "image": "215680092654.dkr.ecr.ap-south-1.amazonaws.com/proman-nginx:latest",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],

      "links": ["backend","client"],
      "memory": 100
    }
  ]
}
