{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "215680092654.dkr.ecr.ap-south-1.amazonaws.com/proman-frontend",
      "hostname": "client",
      "essential": false,
      "memory": 300
    },
    {
      "name": "backend",
      "image": "215680092654.dkr.ecr.ap-south-1.amazonaws.com/proman-backend",
      "hostname": "backend",
      "essential": false,
      "memory": 600
    },
    {
      "name": "nginx",
      "image": "215680092654.dkr.ecr.ap-south-1.amazonaws.com/proman-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],

      "links": ["client", "backend"],
      "memory": 200
    }
  ]
}
