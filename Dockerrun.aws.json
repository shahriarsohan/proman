{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "nginx",
      "image": "sohan0077/dkr-multicontainer-app-nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],

      "links": ["backend"],
      "memory": 1024
    },
    {
      "name": "backend",
      "image": "215680092654.dkr.ecr.ap-south-1.amazonaws.com/proman-backend:latest",
      "hostname": "backend",
      "essential": false,
      "memory": 1024
    }
  ]
}
